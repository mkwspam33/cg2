        <event_name>megaphone.insert.message</event_name>
        <name>Insert Megaphone Message Event</name>
        <order>100</order>
        <script><![CDATA[function insertMegaphoneMessage() {
	//CG2
	
	if(event.parm1.nil()) {
		gs.warn("Empty megaphone broadcast message inserted");
	} 
	else {
		var mega = new MegaphoneBroadcastInsert();
		mega.fromEvent(event.parm1);
	}
}

insertMegaphoneMessage();]]></script>
